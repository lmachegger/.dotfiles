* {
/*background-color can be transparent as well, e.g.: #1B1D2450*/
  background-color: #1B1D24;
  border-color: #1B1D24;
  text-color: #AFB4BA;
  spacing: 0;
  width: 512px;
  width: 812px;
}

inputbar {
  border: 0 0 1px 0;
  children: [entry];
}

prompt {
  padding: 14px;
  border: 0 1px 0 0;
}

textbox {
  background-color: #AFB4BA;
  border: 0 0 1px 0;
  border-color: #282C33;
  padding: 8px 14px;
}

entry {
  padding: 14px;
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
}

element {
  border: 0 0 1px 0;
  padding: 10px;
}

element selected {
  background-color: #3498DB;
  text-color: #FFFFFF;
}

element-icon{
    size: 2ch;
}